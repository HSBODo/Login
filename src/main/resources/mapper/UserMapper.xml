<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.pointman.login.mapper.UserMapper">

    <select id="validationLogin" resultType="site.pointman.login.domain.Member" parameterType="site.pointman.login.domain.Member">
        SELECT * FROM TB_MEMBER WHERE id=#{id} AND pwd =#{pwd}
    </select>

</mapper>